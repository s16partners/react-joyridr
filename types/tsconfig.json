{
  "compilerOptions": {
    "baseUrl": "../",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["es5", "es2015.promise"],
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "target": "es5",
    "typeRoots": ["../"],
    "types": []
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
